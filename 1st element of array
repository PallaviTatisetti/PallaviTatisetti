#include <stdio.h>
int main() {
    int n,i,j,a[100],temp;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
        for(j=i+1;j<n;j++){
            if(a[i]>a[j]){
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
        printf("%d",a[0]);
    return 0;
}

//**********C++ program**************
#include <iostream>
using namespace std;
int main() 
{
    int i,n,a[100];
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }    
    int small=a[0];
    for(i=0;i<n;i++)
    {
        if(a[i]<small)
        {
            small=a[i];
        }
    }
        cout<<small;
    return 0;
}
